{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "mmdiagram.generator",
            "args": [
                "kernel", "0x10", "0x10",
                "kernel", "0x40", "0xDD",
                "dtb", "0xCC", "0xCC",
                "-o", "doc/example/report.md"
            ],
            "justMyCode": false
        }
    ]
}