          "items": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object"
          },
          "title": "Memory Region Links",
          "type": "array"
        }
      },
      "required": [
        "memory_region_name",
        "memory_region_origin",
        "memory_region_size"
      ],
      "title": "MemoryRegion",
      "type": "object"
    }
  },
  "properties": {
    "diagram_name": {
      "description": "The name of the diagram.",
      "title": "Diagram Name",
      "type": "string"
    },
    "memory_maps": {
      "description": "The diagram frame. Can contain many memory maps.",
      "items": {
        "$ref": "#/$defs/MemoryMap"
      },
      "title": "Memory Maps",
      "type": "array"
    }
  },
  "required": [
    "diagram_name",
    "memory_maps"
  ],
  "title": "Diagram",
  "type": "object"
}