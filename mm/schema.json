{
  "$defs": {
    "MemoryMap": {
      "properties": {
        "memory_map_name": {
          "description": "Name of the memory map",
          "title": "Memory Map Name",
          "type": "string"
        },
        "memory_regions": {
          "description": "Memory map containing memory regions.",
          "items": {
            "$ref": "#/$defs/MemoryRegion"
          },
          "title": "Memory Regions",
          "type": "array"
        }
      },
      "required": [
        "memory_map_name",
        "memory_regions"
      ],
      "title": "MemoryMap",
      "type": "object"
    },
    "MemoryRegion": {
      "properties": {
        "name": {
          "description": "Name of the MemoryMap.",
          "title": "Name",
          "type": "string"
        },
        "origin": {
          "description": "Origin address of the MemoryMap. In hex format string.",
          "title": "Origin",
          "type": "string"
        },
        "size": {
          "description": "Size (in bytes) of the MemoryMap. In hex format string.",
          "title": "Size",
          "type": "string"
        },
        "links": {
          "default": [
            {
              "<ParentMemoryMap>": "<ChildMemoryRegion>"
            }
          ],
          "description": "Link to another memory region. E.g. <MemoryMap:name>.<MemoryRegion:Name>",
          "items": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object"
          },
          "title": "Links",
          "type": "array"
        }
      },
      "required": [
        "name",
        "origin",
        "size"
      ],
      "title": "MemoryRegion",
      "type": "object"
    }
  },
  "properties": {
    "diagram_name": {
      "description": "The name of the diagram.",
      "title": "Diagram Name",
      "type": "string"
    },
    "memory_maps": {
      "description": "The diagram frame. Can contain many memory maps.",
      "items": {
        "$ref": "#/$defs/MemoryMap"
      },
      "title": "Memory Maps",
      "type": "array"
    }
  },
  "required": [
    "diagram_name",
    "memory_maps"
  ],
  "title": "Diagram",
  "type": "object"
}