{
  "$defs": {
    "MemoryMap": {
      "properties": {
        "memory_map_name": {
          "description": "Name of the memory map.",
          "title": "Memory Map Name",
          "type": "string"
        },
        "memory_regions": {
          "description": "Memory map containing memory regions.",
          "items": {
            "$ref": "#/$defs/MemoryRegion"
          },
          "title": "Memory Regions",
          "type": "array"
        }
      },
      "required": [
        "memory_map_name",
        "memory_regions"
      ],
      "title": "MemoryMap",
      "type": "object"
    },
    "MemoryRegion": {
      "properties": {
        "memory_region_name": {
          "description": "Name of the MemoryMap.",
          "title": "Memory Region Name",
          "type": "string"
        },
        "memory_region_origin": {
          "description": "Origin address of the MemoryMap. In hex format string.",
          "title": "Memory Region Origin",
          "type": "string"
        },
        "memory_region_size": {
          "description": "Size (in bytes) of the MemoryMap. In hex format string.",
          "title": "Memory Region Size",
          "type": "string"
        },
        "memory_region_links": {
          "default": [],
